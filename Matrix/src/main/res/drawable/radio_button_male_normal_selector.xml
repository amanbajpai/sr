<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_checked="true" android:state_window_focused="false" android:state_enabled="true" android:drawable="@drawable/man_select" />
    <item android:state_checked="false" android:state_window_focused="false" android:state_enabled="true" android:drawable="@drawable/man" />
    <item android:state_checked="true" android:state_pressed="true" android:state_enabled="true" android:drawable="@drawable/man_select_focus" />
    <item android:state_checked="false" android:state_pressed="true" android:state_enabled="true" android:drawable="@drawable/man_press" />
    <item android:state_checked="true" android:state_focused="true" android:state_enabled="true" android:drawable="@drawable/man_select_focus" />
    <item android:state_checked="false" android:state_focused="true" android:state_enabled="true" android:drawable="@drawable/man_focus" />
    <item android:state_checked="false" android:state_enabled="true" android:drawable="@drawable/man" />
    <item android:state_checked="true" android:state_enabled="true" android:drawable="@drawable/man_select" />
    <!-- Disabled states -->
    <item android:state_checked="true" android:state_window_focused="false" android:drawable="@drawable/man_select_dis" />
    <item android:state_checked="false" android:state_window_focused="false" android:drawable="@drawable/man_dis" />
    <item android:state_checked="true" android:state_focused="true" android:drawable="@drawable/man_select_dis" />
    <item android:state_checked="false" android:state_focused="true" android:drawable="@drawable/man_dis" />
    <item android:state_checked="false" android:drawable="@drawable/man_dis" />
    <item android:state_checked="true" android:drawable="@drawable/man_select_dis" />
</selector >